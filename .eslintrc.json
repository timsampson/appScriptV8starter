{
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es2020 ": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "script",
    "project": "./tsconfig.json"
  }
}